                    "type": "string"
                },
                "gender": {
                    "type": "integer",
                    "format": "int64"
                },
                "idCard": {
                    "type": "string"
                },
                "mobile": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "qqToken": {
                    "type": "string"
                },
                "wxToken": {
                    "type": "string"
                }
            }
