    "basePath": "/v1",
    "paths": {
        "/user/getUser/": {
            "get": {
                "tags": [
                    "user"
                ],
                "description": "获取指定用户",
                "operationId": "UserController.根据获取指定用户",
                "parameters": [
                    {
                        "in": "query",
                        "name": "uid",
                        "description": "用户ID",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "\"操作成功\"",
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    }
                }
            }
        },
        "/user/register/": {
            "get": {
                "tags": [
                    "user"
                ],
                "description": "注册接口",
                "operationId": "UserController.注册",
                "parameters": [
                    {
                        "in": "query",
                        "name": "username",
                        "description": "帐号",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "password",
                        "description": "密码",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "schema": {
                            "$ref": "#/definitions/models.Account"
                        }
                    }
                }
            }
        },
        "/user/setUserInfo/": {
            "post": {
                "tags": [
                    "user"
                ],
                "description": "用户信息",
                "operationId": "UserController.用户信息",
                "parameters": [
                    {
                        "in": "header",
                        "name": "utoken",
                        "description": "userToken",
                        "type": "string"
                    },
                    {
                        "in": "body",
                        "name": "body",
                        "description": "body for user content",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "schema": {
                            "$ref": "#/definitions/."
                        }
                    }
                }
            }
        }
    },
    "definitions": {
